{
  "name": "@hapi/hoek",
  "description": "General purpose node utilities",
  "version": "11.0.6",
  "repository": "git://github.com/hapijs/hoek",
  "exports": {
    ".": {
      "default": "./esm/index.mjs",
      "import": "./esm/index.mjs",
      "require": "./cjs/index.js",
      "types": "./lib/index.ts"
    },
    "./applyToDefaults": "./cjs/applyToDefaults.js",
    "./assert": "./cjs/assert.js",
    "./assertError": "./cjs/assertError.js",
    "./bench": "./cjs/bench.js",
    "./block": "./cjs/block.js",
    "./clone": "./cjs/clone.js",
    "./contain": "./cjs/contain.js",
    "./deepEqual": "./cjs/deepEqual.js",
    "./escapeHeaderAttribute": "./cjs/escapeHeaderAttribute.js",
    "./escapeHtml": "./cjs/escapeHtml.js",
    "./escapeJson": "./cjs/escapeJson.js",
    "./escapeRegex": "./cjs/escapeRegex.js",
    "./flatten": "./cjs/flatten.js",
    "./ignore": "./cjs/ignore.js",
    "./intersect": "./cjs/intersect.js",
    "./isPromise": "./cjs/isPromise.js",
    "./merge": "./cjs/merge.js",
    "./once": "./cjs/once.js",
    "./reach": "./cjs/reach.js",
    "./reachTemplate": "./cjs/reachTemplate.js",
    "./stringify": "./cjs/stringify.js",
    "./wait": "./cjs/wait.js"
  },
  "keywords": [
    "utilities"
  ],
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@hapi/code": "^9.0.3",
    "@hapi/eslint-plugin": "^7.0.0",
    "@hapi/lab": "^26.0.0",
    "@types/node": "^22",
    "tsx": "^4.19.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.11.0"
  },
  "scripts": {
    "test": "lab -a @hapi/code -t 100 -L -Y",
    "test-cov-html": "lab -a @hapi/code -t 100 -L -r html -o coverage.html",
    "lint": "tsc --noEmit && eslint . --ext .js,.ts",
    "test:new": "tsx --test --experimental-test-coverage test/**/*.ts",
    "build:cjs": "tsc -p tsconfig.build.json --outDir cjs --module commonjs",
    "build:esm": "tsc -p tsconfig.build.json --outDir esm --module es2015"
  },
  "license": "BSD-3-Clause",
  "packageManager": "pnpm@9.12.0+sha512.4abf725084d7bcbafbd728bfc7bee61f2f791f977fd87542b3579dcb23504d170d46337945e4c66485cd12d588a0c0e570ed9c477e7ccdd8507cf05f3f92eaca"
}